	<a href="/">
		<div class="logo_btn">🕮</div>
	</a>
	<% if(isLogin){ %>
		<a href="/user/logout">
			<div class="logout_btn">로그아웃</div>
		</a>
	<%}else{ %>
		<a href="/user/signup">
			<div class="login_btn">회원가입</div>
		</a>
	<% } %>
	<script>
		$('.menu_btn').click(function(e){
			$('.menu_open-background').toggleClass('unshow');
		})
		$('.menu_open-close').click(function(e){
			$('.menu_open-background').toggleClass('unshow');
		})
	</script>
